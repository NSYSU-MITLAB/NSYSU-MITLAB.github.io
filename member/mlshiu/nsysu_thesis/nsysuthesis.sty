%%%
%%% Command to generate title page.
%%%
\newcommand{\thesistitlepage}{
  \thispagestyle{empty}
  \begin{spacing}{2}
    \begin{center}
      \mbox{}\\[-0.375in]
      \renewcommand\baselinestretch{2}
      \includegraphics[width=1.2in,height=1.2in]{logo/nsysulogo.jpg}\\[0.6in]
      \@universityname\\[0.2in]	    
      \@thesistype\\
	  \@enguniversityname\\
	  \@thesisengtype\\
	  
      \strut\vfill
      \@thesistitlechinese\\
      \mbox{}\\
      \@thesistitleenglish\\
      \mbox{}\\      
      \vfill\strut
      \@thesisstudent\\
	  \@thesisengstudent\\
      \@thesisadvisor\\
	  \@thesisengadvisor\\
      \mbox{}\\
      \@thesisyear\\
	  \@thesisengyear
    \end{center}
  \end{spacing}
  \newpage
}

